			{ "uuid": "b904da82-6aae-4f2e-8b5d-5359cf48939e", "label": "Yes",
                          "metrics": [ { "uuid": "a42bded3-a085-45f8-b384-32b4a77c8385", "measure": 1.0 } ] }
